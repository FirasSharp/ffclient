  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug download",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["--inputFile", "D:\\ff\\l.txt", "--savePath", "D:\\ff"]
    },
    {
      "name": "debug versioning",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": ["-v"]
    },

    {
      "name": "debug test",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/mytest/main.go",
      "args": []
    }
  ]
}
